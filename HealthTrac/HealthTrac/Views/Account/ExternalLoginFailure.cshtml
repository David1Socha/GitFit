@{
    ViewBag.Title = "Login Failure";
}
<head>
    @Styles.Render("~/Content/css")
</head>
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("HealthTrac", "Index", "Home", null, new { @class = "navbar-brand" })
        </div>
    </div>
</div>
<div style="text-align: center; margin-top: 80px">
    <h2>@ViewBag.Title.</h2>
    @if (ViewBag.NoAccountCreated)
    {
        <h3>You must login through the mobile app to create a profile before you can login to the web dashboard.</h3>
    }
    else
    {
        <h3 class="text-error">Unsuccessful login with service.</h3>
    }
</div>


